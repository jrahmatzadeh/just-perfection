#: src/ui/prefs.ui:80
msgid "Select File"
msgstr "Seleccione Archivo"

#: src/ui/prefs.ui:188
msgid "No Results Found"
msgstr "No se han encontrado resultados"

#: src/ui/prefs.ui:210
msgid "Override"
msgstr "Control manual"

#: src/ui/prefs.ui:267
msgid "Shell Theme"
msgstr "Tema de Shell"

#: src/ui/prefs.ui:278
msgid "Overrides the shell theme partially to create a minimal desktop"
msgstr "Anula parcialmente el tema de shell para crear un escritorio mínimo."

#: src/ui/prefs.ui:328
msgid "Visibility"
msgstr "Visibilidad"

#: src/ui/prefs.ui:377
msgid "Panel"
msgstr "Panel"

#: src/ui/prefs.ui:433
msgid "Activities Button"
msgstr "Botón de actividades"

#: src/ui/prefs.ui:444
msgid "Button in panel to toggle overview visibility"
msgstr "Botón en el panel para alternar la visibilidad de la vista general"

#: src/ui/prefs.ui:508
msgid "App Menu"
msgstr "Menú de aplicaciones"

#: src/ui/prefs.ui:519
msgid "The menu button in panel with focused window text"
msgstr "El botón de menú en el panel con el texto de la ventana enfocada"

#: src/ui/prefs.ui:583
msgid "Clock Menu"
msgstr "Menú del reloj"

#: src/ui/prefs.ui:594
msgid "Also known as date menu shows date and time in panel"
msgstr ""
"También conocido como menú de fecha, muestra la fecha y la hora en el panel."

#: src/ui/prefs.ui:658
msgid "Keyboard Layout"
msgstr "Distribución del teclado"

#: src/ui/prefs.ui:669
msgid "Keyboard Layout indicator button in panel"
msgstr "Botón indicador de distribución del teclado en el panel"

#: src/ui/prefs.ui:733
msgid "Accessibility Menu"
msgstr "Menú de accesibilidad"

#: src/ui/prefs.ui:744
msgid "Accessibility Menu indicator button in panel"
msgstr "Botón indicador de menú de accesibilidad en el panel"

#: src/ui/prefs.ui:808
msgid "System Menu (Aggregate Menu)"
msgstr "Menú del sistema (Menú agregado)"

#: src/ui/prefs.ui:819
msgid "System menu holds power, volume and network icons in panel"
msgstr ""
"El menú del sistema contiene los íconos de encendido, volumen y red en el "
"panel"

#: src/ui/prefs.ui:883
msgid "Search"
msgstr "Búsqueda"

#: src/ui/prefs.ui:894
msgid "Search entry in overview"
msgstr "Entrada de búsqueda en la vista general"

#: src/ui/prefs.ui:958
msgid "Dash"
msgstr "Dash"

#: src/ui/prefs.ui:969
msgid "Dash holds favorite and opened applications icons"
msgstr "Dash tiene íconos de aplicaciones favoritas y abiertas"

#: src/ui/prefs.ui:1033
msgid "Show Applications Button"
msgstr "Mostrar botón de aplicaciones"

#: src/ui/prefs.ui:1044
msgid "Button in dash that toggles applications list visibility"
msgstr ""
"Botón en el tablero que alterna la visibilidad de la lista de aplicaciones"

#: src/ui/prefs.ui:1108
msgid "On Screen Display (OSD)"
msgstr "Visualización en pantalla (OSD)"

#: src/ui/prefs.ui:1119
msgid "Volume and brightness on screen display when the change happens"
msgstr "Volumen y brillo en la pantalla cuando ocurre un cambio"

#: src/ui/prefs.ui:1183
msgid "Workspace Popup"
msgstr "Indicador del espacio de trabajo"

#: src/ui/prefs.ui:1194
msgid "Popup that appears on the screen when you change the workspace"
msgstr ""
"Indicador que aparece en la pantalla cuando se cambia el espacio de trabajo"

#: src/ui/prefs.ui:1258
msgid "Workspace Switcher"
msgstr "Conmutador del espacio de trabajo"

#: src/ui/prefs.ui:1269
msgid ""
"Also refers to workspace thumbnails that you see in overview for selecting a "
"workspace"
msgstr ""
"También se refiere a las miniaturas del espacio de trabajo que ve en la "
"vista general para seleccionar un espacio de trabajo."

#: src/ui/prefs.ui:1333
msgid "Workspaces App Grid"
msgstr "Cuadrícula de aplicaciones de espacios de trabajo"

