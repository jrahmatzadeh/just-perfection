#: src/ui/prefs.ui:1098
msgid "System Menu (Aggregate Menu)"
msgstr "stelsel menu"

#: src/ui/prefs.ui:1109
msgid "System menu holds power, volume and network icons in panel"
msgstr "Stelsel menu hou krag, volume en netwerk ikone in paneel"

#: src/ui/prefs.ui:1173
msgid "Search"
msgstr "Soek"

#: src/ui/prefs.ui:1184
msgid "Search entry in overview"
msgstr "Soek inskrywing in oorsig"

#: src/ui/prefs.ui:1248
msgid "Dash"
msgstr "Dash"

