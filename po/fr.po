#: ui/prefs.ui:3386
msgid "Default Speed"
msgstr "Vitesse par défaut"

#: ui/prefs.ui:3387
msgid "Fastest"
msgstr "Le plus rapide"

#: ui/prefs.ui:3388
msgid "Faster"
msgstr "Plus rapide"

#: ui/prefs.ui:3389
msgid "Fast"
msgstr "Rapide"

#: ui/prefs.ui:3390
msgid "Slow"
msgstr "Lent"

#: ui/prefs.ui:3391
msgid "Slower"
msgstr "Plus lent"

#: ui/prefs.ui:3392
msgid "Slowest"
msgstr "Le plus lent"

#: ui/prefs.ui:3432
msgid "Dash Icon Size"
msgstr "Taille des icônes de la barre de raccourcis"

#: ui/prefs.ui:3493
msgid "Notification Banner Position"
msgstr "Position de la bannière de notification"

#: ui/prefs.ui:3504
msgid "Notification popup position when notifications show up on the screen"
msgstr ""
"Position de la fenêtre contextuelle de notification lorsque les "
"notifications s'affichent à l'écran"

#: ui/prefs.ui:3525
msgid "Top Start"
msgstr "Top départ"

#: ui/prefs.ui:3526
